class Solution {
    public String convertToTitle(int columnNumber) {
        
        String result = "";

        while(columnNumber > 0){

            columnNumber--;

            int rem = columnNumber % 26 ;

            char letter = (char) ('A' + rem);

            result = letter + result;

            columnNumber = columnNumber / 26;
        } 
        return result;
    }
}
