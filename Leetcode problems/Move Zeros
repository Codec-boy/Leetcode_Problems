int j = 0;                          
// Pointer to place the next non-zero element
for (int i = 0; i < nums.length; i++) {
    if (nums[i] != 0) {  
        // Found a non-zero
        int temp = nums[i];        
        //Swap nums[i] with nums[j]
        nums[i] = nums[j];
        nums[j] = temp;
        j++;                     
// Move pointer to next available position
    }
}